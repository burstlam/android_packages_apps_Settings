<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Auto keyboard rotation -->
    <string-array name="keyboard_rotation_timeout_entries" translatable="false">
        <item>@string/key_rotation_timeout_2s</item>
        <item>@string/key_rotation_timeout_5s</item>
        <item>@string/key_rotation_timeout_10s</item>
        <item>@string/key_rotation_timeout_20s</item>
        <item>@string/key_rotation_timeout_30s</item>
        <item>@string/key_rotation_timeout_60s</item>
    </string-array>

    <string-array name="keyboard_rotation_timeout_values" translatable="false">
        <item>2000</item>
        <item>5000</item>
        <item>10000</item>
        <item>20000</item>
        <item>30000</item>
        <item>60000</item>
    </string-array>

    <!-- Profile Mode -->
    <string-array name="profile_entries">
        <item>On</item>
        <item>Off</item>
        <item>No override</item>
    </string-array>

    <!-- Values for vibrate_entries matching constants in SoundSettings. Do not translate. -->
    <string-array name="profile_values" translatable="false">
        <item>OVERRIDE</item>
        <item>SUPPRESS</item>
        <item>DEFAULT</item>
    </string-array>

    <string-array name="profile_connection_entries">
        <item>Disable</item>
        <item>Enable</item>
    </string-array>

    <string-array name="profile_connection_values" translatable="false">
        <item>0</item>
        <item>1</item>
    </string-array>

    <string-array name="profile_vibrator_entries" translatable="false">
        <item>@string/vibrator_state_disabled</item>
        <item>@string/vibrator_state_silent</item>
        <item>@string/vibrator_state_enabled</item>
    </string-array>

    <string-array name="profile_vibrator_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <string-array name="profile_lockmode_entries" translatable="false">
        <item>@string/profile_lockmode_default</item>
        <item>@string/profile_lockmode_insecure</item>
        <item>@string/profile_lockmode_disabled</item>
    </string-array>

    <string-array name="profile_lockmode_summaries" translatable="false">
        <item>@string/profile_lockmode_default_summary</item>
        <item>@string/profile_lockmode_insecure_summary</item>
        <item>@string/profile_lockmode_disabled_summary</item>
    </string-array>

    <!-- Values for profile lock mode. Do not translate. -->
    <string-array name="profile_lockmode_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Profile airplane mode options. Do not translate. -->
    <string-array name="profile_airplanemode_entries" translatable="false">
        <item>@string/profile_airplanemode_default</item>
        <item>@string/profile_airplanemode_enabled</item>
        <item>@string/profile_airplanemode_disabled</item>
    </string-array>

    <!-- Profile airplane mode summaries. Do not translate. -->
    <string-array name="profile_airplanemode_summaries" translatable="false">
        <item>@string/profile_airplanemode_default_summary</item>
        <item>@string/profile_airplanemode_enabled_summary</item>
        <item>@string/profile_airplanemode_disabled_summary</item>
    </string-array>

    <!-- Values for profile airplane mode. Do not translate. -->
    <string-array name="profile_airplanemode_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Profile Mode End-->

</resources>
